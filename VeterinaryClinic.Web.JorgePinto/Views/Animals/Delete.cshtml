@model VeterinaryClinic.Web.JorgePinto.Data.Entities.Animal

@{
    ViewData["Title"] = "Delete";
}

<h3>Delete</h3>

<h4>Are you sure you want to delete this Animal?</h4>
<div>

    <partial name="_AnimalsDD" />
    
